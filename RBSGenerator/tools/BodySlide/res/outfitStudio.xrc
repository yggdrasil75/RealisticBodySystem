							<fg>#ffffff</fg>
						</object>
           
					</object>
          <object class="sizeritem">
            <option>1</option>
            <flag>wxBOTTOM|wxLEFT|wxRIGHT|wxEXPAND</flag>
            <border>2</border>
            <object class="wxTreeCtrl" name="outfitBones">
              <style>wxTR_LINES_AT_ROOT|wxTR_HIDE_ROOT | wxTR_NO_LINES|wxTR_HAS_BUTTONS|wxTR_EXTENDED|wxTR_MULTIPLE</style>
              <bg>#404040</bg>
              <fg>#ffffff</fg>
              <hidden>1</hidden>
            </object>
          </object>
					<object class="sizeritem">
						<option>0</option>
						<flag>wxALL|wxEXPAND|wxLEFT</flag>
						<border>2</border>
						<object class="wxPanel" name="m_panel6">
							<style>wxTAB_TRAVERSAL</style>
							<object class="wxBoxSizer">
								<orient>wxHORIZONTAL</orient>
								<object class="sizeritem">
									<option>0</option>
									<flag>wxALL</flag>
									<border>5</border>
									<object class="wxCheckBox" name="refName">
										<fg>#ffffff</fg>
										<enabled>0</enabled>
										<label>Referenced Base Shape</label>
										<checked>1</checked>
									</object>
								</object>
								<object class="sizeritem">
									<option>2</option>
									<flag>wxEXPAND|wxLEFT</flag>
									<border>20</border>
									<object class="wxChoice" name="refShape">
										<enabled>0</enabled>
										<selection>0</selection>
